{
    "name": "konsole-night",
    "displayName": "konsole Night",
    "description": "Dark color theme from Garuda Linux konsole",
    "publisher": "Enoslyznne",
    "icon": "images/banner.png",
    "version": "1.4.3",
    "keywords": [
        "Themes",
        "themes",
        "dark",
        "konsole",
        "konsole night",
        "konsole Night Theme for Konsole Terminal",
        "lyznne",
        "Night",
        "Garuda linux",
        "linux",
        "terminal",
        "toml",
        "gitignore",
        "syntaxhighlting"
    ],
    "author": {
        "name": "Enos Lyznne",
        "url": "https://www.github.com/lyznne",
        "urls": [
            "https://enos.vercel.app"
        ],
        "twitter": "https://www.twitter.com/lyznne"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lyznne/Konsole-night_Theme.git"
    },
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Konsole Night",
                "uiTheme": "vs-dark",
                "path": "./themes/Konsole Night-color-theme.json"
            }
        ],
        "languages": [
            {
                "id": "gitignore",
                "extensions": [
                    ".gitignore",
                    ".vscodeignore"
                ]
            },
            {
                "id": "graphql",
                "aliases": [
                    "GraphQL",
                    "gql"
                ],
                "extensions": [
                    ".graphql"
                ]
            },
            {
                "id": "toml",
                "aliases": [
                    "toml",
                    "TOML"
                ],
                "extensions": [
                    ".toml"
                ]
            }
        ],
        "grammars": [
            {
                "language": "gitignore",
                "scopeName": "source.gitignore",
                "path": "./syntaxes/gitignore.tmLanguage.json"
            },
            {
                "language": "graphql",
                "scopeName": "source.graphql",
                "path": "./syntaxes/graphql.tmLanguage.json"
            },
            {
                "language": "toml",
                "scopeName": "source.toml",
                "path": "./syntaxes/toml.tmLanguage.json"
            }
        ]
    },
    "license": "MIT"
}
